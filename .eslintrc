{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "es6": true,
    "mocha": true,
    "commonjs": true,
    "node": true
  },
  "globals": {
    "$": true,
    "__DEV__": true
  },
  "rules": {
    "arrow-body-style": 0,
    "arrow-parens": 0,
    "block-spacing": 1,
    "brace-style": 0,
    "comma-dangle": 1,
    "consistent-return": 0,
    "func-names": 1,
    "import/first": 0,
    "import/no-extraneous-dependencies": 1,
    "import/prefer-default-export": 0,
    "jsx-quotes": [1, "prefer-single"],
    "no-case-declarations": 1,
    "no-multiple-empty-lines": 1,
    "no-param-reassign": 0,
    "no-redeclare": 0,
    "no-shadow": 0,
    "no-unused-vars": 1,
    "object-curly-spacing": 1,
    "object-shorthand": 1,
    "react/forbid-prop-types": 0,
    "react/jsx-no-bind": 1,
    "react/jsx-quotes": 0,
    "react/prefer-stateless-function": 0,
    "react/require-default-props": 1,
    "react/jsx-boolean-value": 1
  }
}
